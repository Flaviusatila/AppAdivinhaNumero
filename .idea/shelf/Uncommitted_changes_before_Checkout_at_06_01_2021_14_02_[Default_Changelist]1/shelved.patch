 import androidx.appcompat.app.AppCompatActivity
 import android.os.Bundle
+import android.util.Log
+import android.view.View
+import android.widget.EditText
+import android.widget.Toast
 import com.iftm.flavius.evacinas.R
+import com.iftm.flavius.evacinas.dtos.AgendarRetornoDTO
+import com.iftm.flavius.evacinas.dtos.CadastrarDadosCartaoVacinaDTO
+import com.iftm.flavius.evacinas.dtos.ConsultaDTO
+import com.iftm.flavius.evacinas.services.RetrofitService
+import retrofit2.Call
+import retrofit2.Callback
+import retrofit2.Response
 
 class RegistrarProcedimentoRealizadosActivity : AppCompatActivity() {
     override fun onCreate(savedInstanceState: Bundle?) {
         super.onCreate(savedInstanceState)
         setContentView(R.layout.activity_registrar_procedimento_realizados)
     }
+
+    fun registrarprocedimento(view: View) {
+
+        var cpf = (findViewById<EditText>(R.id.et_registra_procedimento_cpf)).text.toString()
+        var usuario = (findViewById<EditText>(R.id.et_registra_procedimento_usuario)).text.toString()
+        var nome = (findViewById<EditText>(R.id.et_registra_procedimento_nome)).text.toString()
+        var procedimento = (findViewById<EditText>(R.id.et_registra_procedimento_procedimentos)).text.toString()
+        var observacoesVacina = (findViewById<EditText>(R.id.et_registra_procedimento_observacoes)).text.toString()
+        var dose = (findViewById<EditText>(R.id.et_registra_procedimento_dose)).text.toString()
+        var dataAplicacao = (findViewById<EditText>(R.id.et_registra_procedimento_data_aplicacao)).text.toString()
+        var lote = (findViewById<EditText>(R.id.et_registra_procedimento_lote)).text.toString()
+        var dataVenc = (findViewById<EditText>(R.id.et_registra_procedimento_data_aplicacao)).text.toString()
+
+        var local = (findViewById<EditText>(R.id.et_registra_procedimento_consulta_local)).text.toString()
+
+        var consulta:ConsultaDTO = ConsultaDTO(dataAplicacao,local,1)
+        var procediementosRealziados:CadastrarDadosCartaoVacinaDTO =
+            CadastrarDadosCartaoVacinaDTO(cpf,usuario,nome,procedimento,observacoesVacina,dose,dataAplicacao,lote,dataVenc,consulta)
+
+        RetrofitService().api?.registrarprocedimento(procediementosRealziados)?.enqueue(object : Callback<CadastrarDadosCartaoVacinaDTO?>{
+          override fun onResponse(
+                call: Call<CadastrarDadosCartaoVacinaDTO?>,
+                response: Response<CadastrarDadosCartaoVacinaDTO?>
+            ) {
+              if (response.code() == 200){
+                  Toast.makeText(this@RegistrarProcedimentoRealizadosActivity,"Usuario Cadastrado com Sucesso",
+                      Toast.LENGTH_LONG).show()
+              }else{
+                  Toast.makeText(this@RegistrarProcedimentoRealizadosActivity,"Erro ao Cadastrar Usuario", Toast.LENGTH_LONG).show()
+              }
+            }
+
+            override fun onFailure(call: Call<CadastrarDadosCartaoVacinaDTO?>, t: Throwable) {
+                Log.e("Erro", "************** erro **********\n"+t?.message.toString())
+            }
+        })
+    }
 }
